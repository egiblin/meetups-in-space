<h1>Create A Meetup!</h1>

<form action="/create" method="post">
  <label for="name">Meetup Name</label>
  <input type="text" id="name" name="name" >
  <br>
  <label for="location">Meetup Location</label>
  <input type="text" id="location" name="location" >
  <br>
  <label for="time">Meetup Time</label>
  <input type="text" id="time" name="time" >
  <br>
  <label for="duration">Meetup Duration</label>
  <input type="text" id="duration" name="duration" >
  <br>
  <label for="date">Meetup Date</label>
  <input type="text" id="date" name="date" >
  <br>
  <label for="description">Meetup Description</label>
  <input type="text" id="description" name="description" >
  <br>
  <input type="submit">
</form>

<a href="/meetups">RETURN TO MEETUPS</a>
